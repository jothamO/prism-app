# Production Environment
NODE_ENV=production
PORT=3000

# Supabase Production
SUPABASE_URL=https://YOUR_PRODUCTION_PROJECT.supabase.co
SUPABASE_ANON_KEY=your_production_anon_key
SUPABASE_SERVICE_KEY=your_production_service_key

# Redis (Docker-managed)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=change_this_redis_password

# Monty Resource Limits
MONTY_MAX_MEMORY_MB=50
MONTY_TIMEOUT_MS=3000
MONTY_MAX_STACK_DEPTH=100

# Security
ALLOW_SELF_IMPROVEMENT=false
PRISM_AGENT_KEY=your_hmac_signing_key

# OpenRouter
OPENROUTER_API_KEY=your_openrouter_key
