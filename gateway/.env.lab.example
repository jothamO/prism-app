# Owner Lab Environment (Experimental)
NODE_ENV=lab
PORT=18791

# Supabase Staging (shared with staging env)
SUPABASE_URL=https://YOUR_STAGING_PROJECT.supabase.co
SUPABASE_ANON_KEY=your_staging_anon_key
SUPABASE_SERVICE_KEY=your_staging_service_key

# Redis (shared with production)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=change_this_redis_password

# Monty Resource Limits (relaxed for testing)
MONTY_MAX_MEMORY_MB=100
MONTY_TIMEOUT_MS=10000
MONTY_MAX_STACK_DEPTH=200

# Security - OpenClaw CAN modify code in this environment
ALLOW_SELF_IMPROVEMENT=true
PRISM_AGENT_KEY=your_hmac_signing_key

# OpenRouter
OPENROUTER_API_KEY=your_openrouter_key
